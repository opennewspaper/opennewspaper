{*
	$TITLE
	$MESSAGE
	$DATA
	$AJAX_URL
	$AJAX_RETUN_URL
	$RETURN_TO_PAGETYPES

	$OK_ICON
	$EDIT_ICON
	$ADD_ICON
	$DELETE_ICON
*}
{if $TITLE}{$TITLE}<br />{/if}
{if $MESSAGE}<span style="color: red;">{$MESSAGE}</span><br /><br />{/if}
{section name=i loop=$DATA}
{if $DATA[i].ACTIVE == 1}
	{$OK_ICON} {$DATA[i].type_name} <a href="{$DATA[i].AJAX_DELETE_URL}">{$DELETE_ICON}</a><br />
{else}
	<a href="{$DATA[i].AJAX_URL}">{$ADD_ICON}</a> {$DATA[i].type_name}<br />
{/if}
{/section}
<br />
<a href="{$AJAX_RETURN_URL}">{$CLOSE_ICON}</a> {$RETURN_TO_PAGETYPES}